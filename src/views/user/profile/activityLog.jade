div.panel-heading Activity log
div.panel-body
	section#cd-timeline.cd-container
		if (!utils.empty(activities))
			for row in activities
				.cd-timeline-block
					.cd-timeline-img.cd-picture
						img(src='/img/timeline-icon.png', alt='Picture')
					.cd-timeline-content
						if (!utils.empty(row.activity))
							h2  #{row.activity.description}
						p #{row.obs}
						//a.cd-read-more(href='#0') Read more
						span.cd-date #{row.tmstp}
